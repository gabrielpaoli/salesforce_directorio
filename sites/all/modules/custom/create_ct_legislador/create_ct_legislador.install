        ),
       'field_l_u_m_por' => array(
            'field_name' => 'field_l_u_m_por',
            'label' => $t('Ültima modificación por'),
            'type' => 'text'
        )

    );
}

function create_ct_legislador_add_custom_fields() {
    foreach (_create_ct_legislador_installed_fields() as $field) {
        field_create_field($field);
    }
    foreach (_create_ct_legislador_installed_instances() as $fieldinstance) {
        $fieldinstance['entity_type'] = 'node';
        $fieldinstance['bundle'] = 'legislador';
        //print_r($fieldinstance);
        field_create_instance($fieldinstance);
    }
}


function  create_ct_legislador_uninstall() {
    $ournewtype = 'legislador';
    $sql = 'SELECT nid FROM {node} n WHERE n.type = :type';
    $result = db_query($sql, array(':type' => $ournewtype));
    $nodeids = array();
    foreach ($result as $row) {
        $nodeids[] = $row->nid;
    }
    node_delete_multiple($nodeids);
    create_ct_legislador_delete_custom_fields();
    node_type_delete($ournewtype);


    //BORRO MENU
    $link_del_menu = 'node/add/' .$ournewtype;
    menu_link_delete(NULL, $link_del_menu);
    menu_rebuild();

    field_purge_batch(500);

}

function create_ct_legislador_delete_custom_fields() {
    foreach (array_keys(_create_ct_legislador_installed_fields()) as $field) {
        field_delete_field($field);
    }
    $instances = field_info_instances('node', 'legislador');
    foreach ($instances as $instance_name => $fieldinstance) {
        field_delete_instance($fieldinstance);
    }
}

